# Cortex MXMCorp - Environment Variables
# Copier ce fichier vers .env et remplir avec vos vraies cl√©s

# API Keys (REQUIS)
OPENAI_API_KEY=sk-...
DEEPSEEK_API_KEY=...
ANTHROPIC_API_KEY=sk-ant-...

# Database (optionnel, defaults dans config.yaml)
DATABASE_PATH=data/cortex.db
VECTOR_DB_PATH=data/vectors
CACHE_PATH=data/cache

# Redis (optionnel)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/cortex.log

# Development
DEBUG=false
MOCK_LLM=false
